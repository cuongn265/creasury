<p id="notice"><%= notice %></p>

<h1>Invoces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoces.each do |invoce| %>
      <tr>
        <td><%= invoce.name %></td>
        <td><%= invoce.value %></td>
        <td><%= link_to 'Show', invoce %></td>
        <td><%= link_to 'Edit', edit_invoce_path(invoce) %></td>
        <td><%= link_to 'Destroy', invoce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoce', new_invoce_path %>
